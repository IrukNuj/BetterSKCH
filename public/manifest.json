{
  "name": "skypeChannelPostFilter",
  "description": "がめんまっぴんく計画",
  "version": "1.0",

  "manifest_version": 3,

  "permissions": ["activeTab", "storage"],
  "action": {
    "default_title": "ここにタイトルを挿入"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["*://*.skypech.com/*"]
    }
  ]
}
